<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="org.ikasan.connector.util.chunking.model.FileChunkHeader" 
    table="FileChunkHeader">

    <id name="id" type="long" column="Id">
      <generator class="identity" />
    </id>
    <property name="sequenceLength" type="long" column="SequenceLength" not-null="true" />
    <property name="internalMd5Hash" type="java.lang.String" column="InternalMd5Hash" not-null="false" />
    <property name="externalMd5Hash" type="java.lang.String" column="ExternalMd5Hash" not-null="false" />
    <property name="fileName" type="java.lang.String" column="FileName" not-null="true" />
    <property name="chunkTimeStamp" type="long" column="ChunkTimeStamp" not-null="true" /> 
    <property name="clientId" type="java.lang.String" column="ClientId" not-null="false" />  


   
  </class>

</hibernate-mapping>
