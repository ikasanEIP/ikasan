<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

    <!-- source ftp system specs -->
    <bean id="sourceFtpUserName" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="sourceFtpPassword" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="sourceFtpPort" class="java.lang.Integer">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="sourceFtpHostName" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="srcDirectory" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    
    <!-- target ftp system specs -->
    <bean id="targetFtpUserName" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="targetFtpPassword" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="targetFtpPort" class="java.lang.Integer">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="targetFtpHostName" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
    <bean id="destDirectory" class="java.lang.String">
        <constructor-arg value="<replace-me>" />
    </bean>
  
    <bean id="jmsUsername" class="java.lang.String">
          <constructor-arg value="unused" />
    </bean>
    <bean id="jmsPassword" class="java.lang.String">
        <constructor-arg value="unused" />
    </bean>  
    
    
    <!-- set this to false if you want ftpPassive -->
    <bean id="ftpActive" class="java.lang.Boolean">
        <constructor-arg value="true"/>
    </bean>
  
    <!-- 
