    <groupId>org.ikasan.demo</groupId>
	<artifactId>demoFileDelivery-ftpSource-jar</artifactId>
	<version>0.7.3</version>
    <packaging>jar</packaging>
	


	<name>demoFileDelivery :: ftpSource : jar</name>

	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>

		<filters>
			<!-- <filter>../project.properties</filter> -->
			<filter>./module.properties</filter>
		</filters>
    	<plugins>
      		<plugin>
        		<groupId>org.apache.maven.plugins</groupId>
        		<artifactId>maven-compiler-plugin</artifactId>
        		<configuration>
          			<source>1.5</source>
          			<target>1.5</target>
        		</configuration>
                
      		</plugin>


      	</plugins>
	</build>
  <scm>
    <connection>scm:svn:https://ikasaneip.svn.sourceforge.net/svnroot/ikasaneip/tags/ikasan-project-0.7.3</connection>
  </scm>
    
  <distributionManagement>
      <repository>
          <id>ikasaneip-releases</id>
          <name>Ikasan EIP Releases Repository</name>
          <url>http://oss.sonatype.org/content/repositories/ikasaneip-releases</url>
      </repository>
      <snapshotRepository>
          <id>ikasaneip-snapshots</id>
          <name>Ikasan EIP Snapshot Repository</name>
          <url>http://oss.sonatype.org/content/repositories/ikasaneip-snapshots/</url>
      </snapshotRepository>
  </distributionManagement>
    
    
</project>

