<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">    
    
    <property name="dateType" value="datetime" dbms="h2, mssql, sybase"/>
    <property name="dateType" value="timestamp" dbms="mysql"/>
    
    <property name="getDate" value="current_date()" dbms="h2"/>
    <property name="getDate" value="getdate()" dbms="mssql, sybase"/>
    <property name="getDate" value="current_timestamp" dbms="mysql"/>
    
    <property name="identity" value="IDENTITY" dbms="h2"/>
    <property name="identity" value="numeric(18)" dbms="mssql, sybase"/>
    <property name="identity" value="BIGINT" dbms="mysql"/>
    
    <property name="autoincrement" value="false" dbms="h2"/>
    <property name="autoincrement" value="true" dbms="mysql, mssql, sybase"/>
    
    <!-- File transfer table creation -->    
	<include file="db/tables/db-changeLog-createFTChecksumCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTCleanupChunksCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTDeliverBatchCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTDeliverFileCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTFileChunkHeader.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTFileChunk.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTFileFilter.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTRetrieveFileCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTTransactionalResourceCommand.xml" relativeToChangelogFile="true" />
	<include file="db/tables/db-changeLog-createFTXid.xml" relativeToChangelogFile="true" />

	<!-- File transfer cardinality creation -->   
	<include file="db/cardinality/db-changeLog-createFTFileChunkFk.xml" relativeToChangelogFile="true" />
	<include file="db/cardinality/db-changeLog-createFTTransactionalResourceCommandFk.xml" relativeToChangelogFile="true" />
</databaseChangeLog>