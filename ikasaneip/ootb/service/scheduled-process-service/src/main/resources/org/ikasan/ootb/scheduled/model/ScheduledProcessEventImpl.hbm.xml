<!--
    This is the hibernate mapping configuration for ScheduledProcessEvent persistence.
    @author Ikasan Development Team
 -->
<hibernate-mapping>

    <class
        name="org.ikasan.ootb.scheduled.model.ScheduledProcessEventImpl"
        table="ScheduledProcessEvent">

        <id name="id" type="java.lang.Long" column="Id">
            <generator class="identity" />
        </id>

        <property name="agentName"
                  type="java.lang.String"
                  column="AgentName"
                  not-null="true" />
        <property name="agentHostname"
                  type="java.lang.String"
                  column="AgentHostName"
                  not-null="false" />
        <property name="jobName"
                  type="java.lang.String"
                  column="JobName"
                  not-null="true" />
        <property name="jobGroup"
                  type="java.lang.String"
                  column="JobGroup"
                  not-null="true" />
        <property name="jobDescription"
                  type="java.lang.String"
                  column="JobDescription"
                  not-null="false" />
        <property name="commandLine"
                  type="java.lang.String"
                  column="CommandLine"
                  not-null="false" />
        <property name="returnCode"
                  type="int"
                  column="ReturnCode"
                  not-null="false" />
        <property name="successful"
                  type="boolean"
                  column="Successful"
                  not-null="false" />
        <property name="outcome"
                  column="Outcome"
                  not-null="false">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">org.ikasan.ootb.scheduled.model.Outcome</param>
            </type>
        </property>
        <property name="resultOutput"
                  type="java.lang.String"
                  column="ResultOutput"
                  not-null="false" />
        <property name="resultError"
                  type="java.lang.String"
                  column="ResultError"
                  not-null="false" />
        <property name="pid"
                  type="long"
                  column="Pid"
                  not-null="false" />
        <property name="user"
                  type="java.lang.String"
                  column="User"
                  not-null="false" />
        <property name="fireTime"
                  type="long"
                  column="FireTime"
                  not-null="true" />
        <property name="completionTime"
                  type="long"
                  column="CompletionTime"
                  not-null="true" />
        <property name="nextFireTime"
                  type="long"
                  column="NextFireTime"
                  not-null="false" />
        <property name="harvested"
                  type="boolean"
                  column="Harvested"
                  not-null="true" />
        <property name="harvestedDateTime"
                  type="long"
                  column="HarvestedDateTime"
                  not-null="true" />
    </class>

</hibernate-mapping>
