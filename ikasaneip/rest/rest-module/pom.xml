        <dependency>
            <groupId>org.ikasan</groupId>
            <artifactId>ikasan-spec-event</artifactId>
        </dependency>

        <dependency>
            <groupId>org.ikasan</groupId>
            <artifactId>ikasan-module</artifactId>
        </dependency>

		<dependency>
