    <util:constant id="securityMappingAdmin" static-field="org.ikasan.dashboard.ui.framework.constants.SecurityConstants.MAPPING_ADMIN"/>

    <util:list id="editMappingSecurity">
        <ref local="securityMappingWrite"/>
