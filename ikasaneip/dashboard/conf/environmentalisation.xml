<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:util="http://www.springframework.org/schema/util"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">
	
	<bean id="mappingConfigurationExportSchema"
       class="java.lang.String">
        <constructor-arg value="${xml.remote.url}/mappingConfigurationUI/static/org/ikasan/mappingConfigurationUI/mappingConfigurationImportExport.xsd" />
    </bean>
	
	<bean id="mappingConfigurationValuesExportSchema"
       class="java.lang.String">
        <constructor-arg value="${xml.remote.url}/mappingConfigurationUI/static/org/ikasan/mappingConfigurationUI/mappingConfigurationValuesImportExport.xsd" />
    </bean>

	<bean id="ldapServerUrl" class="java.lang.String">
        <constructor-arg value="${ldap.server.url}" />
    </bean>

    <bean id="principalPrefix" class="java.lang.String">
        <constructor-arg value="${principal.prefix}" />
    </bean>
</beans>