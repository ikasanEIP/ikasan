<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Ikasan Framework Properties</comment>
    <entry key="contextImpl.class">org.ikasan.framework.context.DefaultContext</entry>
    <entry key="persistenceImpl.class">org.ikasan.framework.persistence.HibernatePersistence</entry>
    <entry key="xmlParserImpl.class">org.ikasan.framework.xml.parser.DefaultXMLParser</entry>
    <entry key="xmlTransformerImpl.class">org.ikasan.framework.xml.transform.DefaultDOMSerializer</entry>
    <entry key="stringTransformerImpl.class">org.ikasan.framework.transform.DefaultStringTransformer</entry>
    <entry key="xslTransformerImpl.class">org.ikasan.framework.xml.transform.DefaultXSLTransformer</entry>
    <entry key="environmentImpl.class">org.ikasan.framework.util.Env</entry>
    <entry key="payloadImpl.class">org.ikasan.common.component.DefaultPayload</entry>
    <entry key="envelopeImpl.class">org.ikasan.common.component.DefaultEnvelope</entry>
    <entry key="flowImpl.class">org.ikasan.framework.component.DefaultFlow</entry>

    <!-- Ikasan common Appln server Datasources and factories -->
    <entry key="transaction.manager">java:/TransactionManager</entry>
    <entry key="user.transaction">UserTransaction</entry>
    <entry key="jms.xa.connectionFactory">java:/JmsXA</entry>
    <entry key="jms.provider">java:/DefaultJMSProvider</entry>
    <entry key="ds.session.factory">java:/ikasan/framework/default/hibernate/SessionFactory</entry>
    <entry key="localds.session.factory">java:/ikasan/framework/defaultLocal/hibernate/SessionFactory</entry>
    <entry key="xads.session.factory">java:/ikasan/framework/defaultXA/hibernate/SessionFactory</entry>

