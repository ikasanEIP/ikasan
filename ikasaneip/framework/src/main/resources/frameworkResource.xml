    <entry key="component.env">java:comp/env</entry>
    <entry key="component.group.name">java:comp/env/componentGroupName</entry>
    <entry key="component.status.url">java:comp/env/componentStatusURL</entry>
    <entry key="component.name">java:comp/env/componentName</entry>
    <entry key="component.exception.resolver">java:comp/env/exceptionResolver</entry>
    <entry key="component.security.conf.resource">java:comp/env/securityConf</entry>

    <!--  Ikasan framework flow properties. -->
    <entry key="flow.name">java:comp/env/flowClassName</entry>
    <entry key="flow.method">java:comp/env/flowMethodName</entry>
    <entry key="flow.exception.name">-exceptionHandler</entry>
    <entry key="flow.exception.method">onException</entry>
    <entry key="flow.destination.name">java:comp/env/flowDestination</entry>

    <!-- Ikasan framework submission router - deprecated. -->
    <entry key="submit.handler.router">submitRouter</entry>

    <!-- Ikasan framework constants. -->
    <entry key="undefined.value">undefined</entry>

</properties>